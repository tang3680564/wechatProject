<!--miniprogram/pages/my/myIndex.wxml-->
<view class="myView">
  <view class="myTopView">
    <view class="myTopLeftView">
      <view class="myImage">
        <open-data type="userAvatarUrl"></open-data>
      </view>
    </view>
    <view class="myTopRightView">
      <open-data type="userNickName"></open-data>
    </view>
  </view>
  
  <scroll-view scroll-y="true" >
    <view class = "cellView" bindtap="navMyOrderList">
      <view class="cellTextView">我的订单</view>
      <view class="cellSpaceView"> </view>
      <image src="../../images/arrow-right.png"></image>
    </view>
    <view class="cellLineView"></view>

  </scroll-view>
</view>
